{
  "name": "conventional-changelog-gulp",
  "version": "0.1.0",
  "description": "Conventional Changelog preset for gulp projects",
  "author": "Gulp Team <team@gulpjs.com> (http://gulpjs.com/)",
  "contributors": [],
  "repository": "gulpjs/conventional-changelog-gulp",
  "license": "MIT",
  "engines": {
    "node": ">= 0.10"
  },
  "main": "index.js",
  "files": [
    "index.js",
    "templates",
    "convention.md"
  ],
  "scripts": {
    "lint": "eslint . && jscs index.js test/",
    "pretest": "npm run lint",
    "test": "mocha --async-only",
    "cover": "istanbul cover _mocha --report lcovonly",
    "coveralls": "npm run cover && istanbul-coveralls"
  },
  "dependencies": {
    "lodash.trim": "^4.4.0",
    "q": "^1.4.1"
  },
  "devDependencies": {
    "conventional-changelog-core": "^1.5.0",
    "eslint": "^1.7.3",
    "eslint-config-gulp": "^2.0.0",
    "expect": "^1.19.0",
    "istanbul": "^0.4.3",
    "istanbul-coveralls": "^1.0.3",
    "jscs": "^2.3.5",
    "jscs-preset-gulp": "^1.0.0",
    "mocha": "^2.4.5",
    "shelljs": "^0.7.0",
    "through2": "^2.0.1"
  },
  "keywords": [
    "conventional-changelog",
    "gulp",
    "preset"
  ]
}
